    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define",
                "APP_ENV=debug",
                "--dart-define",
                "APP_FLAVOR=MaxMoney",
                "--dart-define",
                "APP_PACKAGE_NAME=com.maxmoney.quantstrategy.dev",
                "--dart-define",
                "APP_NAME=MaxMoney",
                "--dart-define",
                "APP_LAUNCH_ICON=@mipmap/maxmoney_logo",
                "--dart-define",
                "APP_LAUNCH_IMAGE=MAX Money",
            ]
        },
        {
            "name": "Run Test",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "ENV=test"
            ]
        },
        {
            "name": "Run Prod",
            "request": "launch",
            "type": "dart", 
            "args": [
                "--dart-define",
                "ENV=prod"
            ]
        }
    ]
}